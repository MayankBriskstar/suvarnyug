@{
    ViewData["Title"] = "Page Not Found";
}

<div class="container text-center">
    <h1>401 - Page Not Found</h1>
    <img src="~/images/401 Error Unauthorized-pana.svg" alt="404 Not Found" class="img-fluid" style="height:30%; width:30%"/>
    <p>Sorry, the page you are looking for does not exist or you are not authorized to access it.</p>
    <a href="@Url.Action("dashboard", "home")" class="theme-btn style-two bgc-secondary mb-30"><span data-hover="Go to Home">Go to Home</span></a>
</div>
